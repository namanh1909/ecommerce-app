{
  "name": "myapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "init-project": "react-native eject && npm run pod-install",
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "jest",
    "codepush": "./code-push.command",
    "pod-install": "bundle install && cd ios && bundle exec pod install && cd ..",
    "postinstall": "patch-package && yarn pod-install",
    "assets-link": "npx react-native link",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:ts": "tslint 'src/**/*.{ts,tsx,js}'",
    "lint-fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:husky": "eslint $(git diff --staged --name-only | grep -E '(.js$|.ts$|.tsx$)')",
    "devtools": "react-devtools",
    "get-tree": "tree -I 'android|ios|node_modules'",
    "no-edit": "git add . && git commit --amend --no-edit && git push origin HEAD -f",
    "no-verify": "git add . && git commit --amend --no-edit --no-verify && git push origin HEAD -f",
    "rebase": "git checkout develop && git pull origin develop && git checkout - && git rebase -",
    "buildAndroid": "cd android && ./gradlew assembleRelease&& adb -d install app/build/outputs/apk/release/app-release.apk",
    "bundleIOS": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "android-run-debug-develop": "yarn clean-android && react-native run-android --mode=devDebug --appId 'com.doctorbookingapp.dev'",
    "android-run-debug-staging": "yarn clean-android && react-native run-android --mode=stagingDebug --appId 'com.doctorbookingapp.staging'",
    "android-run-debug-product": "yarn clean-android && react-native run-android --mode=productDebug --appId 'com.doctorbookingapp.production'",
    "android-run-release-develop": "cd android && rm -rf build && rm -rf android/build && cd .. && react-native run-android --mode=devRelease --appId 'com.doctorbookingapp.dev'",
    "android-run-release-staging": "react-native run-android --mode=stagingRelease --appId 'com.doctorbookingapp.staging'",
    "android-run-release-product": "react-native run-android --mode=productRelease --appId 'com.doctorbookingapp.production'",
    "android-build-apk-develop": "rm -rf build && rm -rf android/build && export ENVFILE=.env.development && cd android && ./gradlew assembleDevRelease && cd ..",
    "android-build-apk-staging": "rm -rf build && rm -rf android/build && export ENVFILE=.env.staging && cd android && ./gradlew assembleStagingRelease && cd ..",
    "android-build-apk-product": "rm -rf build && rm -rf android/build && export ENVFILE=.env.production && cd android && ./gradlew clean && ./gradlew assembleProductRelease && cd ..",
    "android-build-aab-product": "rm -rf build && rm -rf android/build && export ENVFILE=.env.production && cd android && ./gradlew clean && ./gradlew bundleProductRelease && cd ..",
    "open-folder-apk": "open ./android/app/build/outputs/apk",
    "open-folder-aab": "open ./android/app/build/outputs/bundle",
    "commit": "cz",
    "clean-auto": "react-native clean-project-auto",
    "clean-all": "react-native-clean-project",
    "clean-android": "cd android && ./gradlew clean && cd ..",
    "clean-ios": "rm -rf ios/build && rm -rf ios/Pods && pod cache clean --all && rm -rf ~/.cocoapods"
  },
  "dependencies": {
    "@hookform/resolvers": "3.0.0",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/netinfo": "^11.3.2",
    "@react-native-masked-view/masked-view": "^0.3.1",
    "@react-navigation/bottom-tabs": "^6.6.0",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/stack": "^6.3.29",
    "@reduxjs/toolkit": "^2.2.6",
    "@tanstack/react-query": "^5.31.0",
    "@types/lodash": "^4.17.6",
    "axios": "^1.7.2",
    "i18next": "^23.11.2",
    "jwt-decode": "^4.0.0",
    "ky": "^1.2.4",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-fast-compare": "^3.2.2",
    "react-hook-form": "7.43.7",
    "react-i18next": "^14.1.0",
    "react-native": "0.74.1",
    "react-native-config": "^1.5.2",
    "react-native-device-info": "^11.1.0",
    "react-native-fbsdk-next": "^13.0.0",
    "react-native-gesture-handler": "^2.17.1",
    "react-native-mmkv": "^2.12.2",
    "react-native-permissions": "^4.1.5",
    "react-native-portalize": "^1.0.7",
    "react-native-reanimated": "^3.9.0-rc.1",
    "react-native-root-siblings": "^5.0.1",
    "react-native-safe-area-context": "^4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-svg": "^15.3.0",
    "react-redux": "^9.1.2",
    "reactotron-redux": "^3.1.9",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.3.0",
    "yup": "^1.4.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.74.83",
    "@react-native/eslint-config": "0.74.83",
    "@react-native/metro-config": "0.74.83",
    "@react-native/typescript-config": "0.74.83",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^12.1.2",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/parser": "^6.13.1",
    "babel-jest": "^29.6.3",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-root-import": "^6.6.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "reactotron-react-native": "^5.1.6",
    "reactotron-react-native-mmkv": "^0.2.6",
    "reactotron-react-query": "^1.0.4",
    "typescript": "5.1.3"
  },
  "engines": {
    "node": ">=18"
  }
}
